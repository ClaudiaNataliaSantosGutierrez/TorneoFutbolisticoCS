@page
@model TorneoFutbolistico.App.Frontend.Pages.Municipios.IndexModel
@{
}
<h1>Listado de Municipios</h1>
<table class="table">
  <thead class="thead-dark">
    <tr>
      <th scope="col">Id</th>
      <th scope="col">NombreMunicipio</th>
    </tr>
  </thead>
  <tbody>
      @foreach (var Municipio in Model.Municipios)
      {
          <tr>
              <td>@Municipio.Id</td>
              <td>@Municipio.NombreMunicipio</td>
          </tr>
      }
  </tbody>
</table>