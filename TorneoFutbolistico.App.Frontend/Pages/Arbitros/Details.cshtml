@page
@model TorneoFutbolistico.App.Frontend.Pages.Arbitros.DetailsModel
@{
}
<h1>Detalles Arbitros</h1>
<table class="table table-dark">
    <tr>
        <th scope="col">Nombre</th>
        <td>@Model.arbitro.Nombre</td>
    </tr>
    <tr>
        <th scope="col">Apellidos</th>
        <td>@Model.arbitro.Apellidos</td>
    </tr>
    <tr>
        <th scope="col">NumeroTelefono</th>
        <td>@Model.arbitro.NumeroTelefono</td>
    </tr>
    <tr>
        <th scope="col">Documento</th>
        <td>@Model.arbitro.Documento</td>
    </tr>
    <tr>
        <th scope="col">Id_A</th>
        <td>@Model.arbitro.Id_A</td>
    </tr>
    <tr>
        <th scope="col">Colegio</th>
        <td>@Model.arbitro.Colegio</td>
    </tr>
</table>
<a class="btn btn-primary" asp-page="./Index">Volver</a>