@page
@using TorneoFutbolistico.App.Dominio
@model TorneoFutbolistico.App.Frontend.Pages.DirectoresTecnicos.CreateModel
@{
}
<h1>Nuevo DirectorTecnico</h1>
<form method="post">
    <div class="form-group">
        <label asp-for="directorTecnico.Nombre"></label>
        <input asp-for="directorTecnico.Nombre" class="form-control">
        <span asp-validation-for="directorTecnico.Nombre" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="directorTecnico.Apellidos"></label>
        <input asp-for="directorTecnico.Apellidos" class="form-control">
        <span asp-validation-for="directorTecnico.Apellidos" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="directorTecnico.Documento"></label>
        <input asp-for="directorTecnico.Documento" class="form-control">
        <span asp-validation-for="directorTecnico.Documento" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="directorTecnico.NumeroTelefono"></label>
        <input asp-for="directorTecnico.NumeroTelefono" class="form-control">
        <span asp-validation-for="directorTecnico.Documento" class="text-danger"></span>
    </div>
    <button type="submit" class="btn btn-primary">Crear DirectorTecnico</button>
</form>
<a class="btn btn-secondary" asp-page="./Index">Volver</a>
